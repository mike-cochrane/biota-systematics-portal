{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://logger.landcareresearch.co.nz/cis",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "SystematicsPortal.Web",
      "Environment": "Local"
    }
  },
  "AppSettings": {
    "IdentityServer": {
      "Url": "https://test-identityserver.landcareresearch.co.nz",
      "ClientId": "",
      "ClientSecret": ""
    },
    "AccessService": {
      "Url": "http://localhost:29578/",
      "Name": "dev-SystematicsPortal.Web.Api"
    },
    "Email": {
      "SenderAddress": "",
      "SMTPServer": "smtp.landcareresearch.co.nz",
      "ToAddress": "informaticssupport@landcareresearch.co.nz"
    }
  }
}