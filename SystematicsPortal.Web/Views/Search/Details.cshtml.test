            }

            @if (concept.Orthography != null)
            {
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">@Html.LabelFor(concept => concept.Orthography, "Orthography")</label>
                    <div class="col-sm-10">
                        @Html.Raw(concept.Orthography.Value)
                    </div>
                </div>
            }

            @*@if (concept.Keywords != null)
                {
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Keywords</label>
                        <div class="col-sm-10">
                            @foreach (SystematicsPortal.Web.Models.DocumentKeywords keyword in concept.Keywords)
                            {
                                @Html.Raw(keyword.Keyword.Value)<br />
                            }
                        </div>
                    </div>
                }*@

            @if (concept.RelatedConcepts != null)
            {
                <h4>Associations</h4>
                <div class="form-group row">
                    @foreach (SystematicsPortal.Web.Models.DocumentRelatedConcepts relatedConcept in concept.RelatedConcepts)
                    {
                        if (relatedConcept.type == "has parent")
                        {
                            <label class="col-sm-2 col-form-label">Cited Parent</label>
                            <div class="col-sm-10">
                                <a href="">@Html.Raw(relatedConcept.RelatedTaxon.Value)</a>
                            </div>
                        }
                        if (relatedConcept.type == "has current name")
                        {
                            <label class="col-sm-2 col-form-label">Cited Current Name</label>
                            <div class="col-sm-10">
                                @Html.Raw(relatedConcept.RelatedTaxon.Value)
                            </div>
                        }
                        if (relatedConcept.type == "has alternative name")
                        {
                            <label class="col-sm-2 col-form-label">Cited Alternative Name</label>
                            <div class="col-sm-10">
                                @Html.Raw(relatedConcept.RelatedTaxon.Value)
                            </div>
                        }
                        if (relatedConcept.type != "has parent" && relatedConcept.type != "has current name" && relatedConcept.type != "has alternative name")
                        {
                            <label class="col-sm-2 col-form-label">Cited Association</label>
                            <div class="col-sm-10">
                                @Html.Raw(relatedConcept.RelatedTaxon.Value)
                            </div>
                        }
                    }
                </div>

                <h4>Relationships</h4>
                <div class="form-group row">
                    @foreach (SystematicsPortal.Web.Models.DocumentRelatedConcepts relatedConcept in concept.RelatedConcepts)
                    {
                        if (relatedConcept.RelatedTaxon.type == "is congruent" || relatedConcept.RelatedTaxon.type == "is included in" || relatedConcept.RelatedTaxon.type == "overlaps with" || relatedConcept.RelatedTaxon.type == "is excluded from")
                        {
                            <label class="col-sm-2 col-form-label">Taxon</label>
                            <div class="col-sm-10">
                                @Html.Raw(relatedConcept.RelatedTaxon.Value)
                            </div>
                        }

                        if (relatedConcept.type != null)
                        {
                            <label class="col-sm-2 col-form-label">Relationship</label>
                            <div class="col-sm-10">
                                @Html.Raw(relatedConcept.RelatedTaxon.Value)
                            </div>
                        }
                    }
                </div>

                @if (concept.Descriptions != null)
                {
                    <h4>Descriptions</h4>
                    <div class="form-group row">
                        @foreach (SystematicsPortal.Web.Models.DocumentDescription description in concept.Descriptions)
                        {
                            if (description.Name != null)
                            {
                                <label class="col-sm-2 col-form-label">Name</label>
                                <div class="col-sm-10">
                                    @Html.Raw(description.Name.Value)
                                </div>
                            }
                            if (description.DescriptionText != null)
                            {
                                <label class="col-sm-2 col-form-label">Description</label>
                                <div class="col-sm-10">
                                    @Html.Raw(description.DescriptionText.Value)
                                </div>
                            }
                            if (description.type != null)
                            {
                                <label class="col-sm-2 col-form-label">Type</label>
                                <div class="col-sm-10">
                                    @Html.Raw(description.type)
                                </div>
                            }
                            if (description.category != null)
                            {
                                <label class="col-sm-2 col-form-label">Category</label>
                                <div class="col-sm-10">
                                    @Html.Raw(description.category)
                                </div>
                            }
                            if (description.DescriptionUrl != null)
                            {
                                <label class="col-sm-2 col-form-label">&nbsp;</label>
                                <div class="col-sm-10">
                                    <a href="">@Html.Raw(description.DescriptionUrl.Value)</a>
                                </div>
                            }
                        }
                    </div>
                }
            }
        }
    }

    @if (Model.BiostatusValues != null)
    {
        <h3>Biostatus Values</h3>
        @foreach (SystematicsPortal.Web.Models.DocumentBiostatusValue biostatusValue in Model.BiostatusValues)
        {
            if (!biostatusValue.isActive)
            {
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Origin</label>
                    <div class="col-sm-10">
                        @Html.Raw(biostatusValue.Origin.Value)
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Occurrence</label>
                    <div class="col-sm-10">
                        @Html.Raw(biostatusValue.Occurrence.Value)
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Georegion</label>
                    <div class="col-sm-10">
                        @Html.Raw(biostatusValue.Georegion.Value)
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Reference</label>
                    <div class="col-sm-10">
                        <a href="">@Html.Raw(biostatusValue.BiostatusReference.Text[0].Replace("+=", "<i>").Replace("=+", "</i>"))</a>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Note</label>
                    <div class="col-sm-10">
                        @Html.Raw(biostatusValue.Comment.Value)
                    </div>
                </div>
            }
        }
    }

    <h3>Links</h3>

    @if (Model.HyperLinks != null)
    {
        @foreach (SystematicsPortal.Web.Models.DocumentHyperlink hyperlink in Model.Hyperlinks)
        {
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">&nbsp;</label>
                <div class="col-sm-10">
                    <a href="@Html.Raw(hyperlink.LinkUrl.Value)">@Html.Raw(hyperlink.Caption.Value)</a>
                </div>
            </div>
        }
    }

    @if (Model.ExternalLinks != null)
    {
        <h4>Nomenclators</h4>
        @foreach (SystematicsPortal.Web.Models.DocumentExternalLink externalLink in Model.ExternalLinks)
        {
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">&nbsp;</label>
                <div class="col-sm-10">
                    <a href="@Html.Raw(externalLink.externalId)">@Html.Raw(externalLink.ExternalFullName.Value)</a>
                </div>
            </div>
        }
    }

    <!-- TODO: add Identification Keys -->

    @if (Model.CollectionObjects != null)
    {
        <h3>Collections</h3>
        @foreach (SystematicsPortal.Web.Models.DocumentCollectionObject collectionObject in Model.CollectionObjects)
        {
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">Accession Number</label>
                <div class="col-sm-10">
                    @Html.Raw(collectionObject.accessionNumber)
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">Determined Name</label>
                <div class="col-sm-10">
                    @Html.Raw(collectionObject.determinedName)
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">Collector</label>
                <div class="col-sm-10">
                    @Html.Raw(collectionObject.Collector)
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">Collection Date</label>
                <div class="col-sm-10">
                    @Html.Raw(collectionObject.collectionDateISO)
                </div>
            </div>
            @if (Model.CollectionObjects != null)
            {
                @foreach (SystematicsPortal.Web.Models.DocumentCollectionObjectCollectionEventRegion collectionEventRegion in collectionObject.CollectionEventRegions)
                {
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Country</label>
                        <div class="col-sm-10">@Html.Raw(collectionEventRegion.georegion)</div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Region</label>
                        <div class="col-sm-10">@Html.Raw(collectionEventRegion.georegionSchema)</div>
                    </div>
                }
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Host</label>
                    <div class="col-sm-10">@Html.Raw(collectionObject.host)</div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Substrate</label>
                    <div class="col-sm-10">@Html.Raw(collectionObject.substrate)</div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Type status</label>
                    <div class="col-sm-10">@Html.Raw(collectionObject.typeStatus)</div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">&nbsp;</label>
                    <div class="col-sm-10">TODO: add image and meta data</div>
                </div>
            }
        }
    }

    @if (Model.nameId)
    {
        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Name Identifier</label>
            <div class="col-sm-10">
                @Html.Raw(Model.nameId)
            </div>
        </div>
    }
}

@if (Model.added)
{
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Date Added</label>
        <div class="col-sm-10">
            @Html.Raw(Model.added)
        </div>
    </div>
}

@if (Model.updated)
{
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Date Updated</label>
        <div class="col-sm-10">
            @Html.Raw(Model.updated)
        </div>
    </div>
}

@if (Model.source)
{
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Source database</label>
        <div class="col-sm-10">
            @Html.Raw(Model.source)
        </div>
    </div>
}
