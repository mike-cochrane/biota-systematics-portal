@if (Model.Class.Equals("reference"))
{
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">@Html.LabelFor(model => model.Title, "Title")</label>
        <div class="col-sm-10">
            @Html.Raw(Model.Title)
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">@nbsp;</label>
        <div class="col-sm-10">
            <a href="">@Html.Raw(Model.ParentReference)</a>
        </div>
    </div>

    @if (Model.Fields != null)
    {
        <h3>Fields</h3>
        <div class="form-group row">
            @foreach (SystematicsPortal.Web.Models.DocumentFields field in Model.Fields)
            {
                <label class="col-sm-2 col-form-label">@field.type</label>
                <div class="col-sm-10">
                    @if (field.type == "DOI")
                    {
                        <a href="">
                            @Html.Raw(field.Value)
                        </a>
                    }
                    else
                    {
                        @Html.Raw(field.Value)
                    }
                </div>
            }
        </div>
    }

    <h3>Concepts</h3>
    @if (Model.DefinedConcepts != null)
    {
        <div class="form-group row">
            @foreach (SystematicsPortal.Web.Models.DocumentDefinedConcepts definedConcept in Model.DefinedConcepts)
            {
                <label class="col-sm-2 col-form-label">Cited Name</label>
                <div class="col-sm-10">
                    <a href="">@Html.Raw(definedConcept.nameFull)</a>
                </div>
                <label class="col-sm-2 col-form-label">TODO: add icon</label>
                <div class="col-sm-10">
                    <a href="">@Html.Raw(definedConcept.conceptId)<a />
                </div>
            }
        </div>
    }

    @if (Model.Associations != null)
    {
        <h4>Cited Taxon</h4>
        <div class="form-group row">
            @foreach (SystematicsPortal.Web.Models.DocumentAssociations association in Model.Associations)
            {
                <label class="col-sm-2 col-form-label">Associated taxon</label>
                <div class="col-sm-10">
                    <a href="">@Html.Raw(association.relatedName)</a>
                </div>
                <label class="col-sm-2 col-form-label">Association type</label>
                <div class="col-sm-10">
                    @Html.Raw(association.type)
                </div>
                <label class="col-sm-2 col-form-label">TODO: add icon</label>
                <div class="col-sm-10">
                    <a href="">@Html.Raw(association.relatedConceptId)</a>
                </div>
            }
        </div>
    }

    @if (Model.CitedTaxa != null)
    {
        <h4>Other taxa cited</h4>
        <div class="form-group row">
            @foreach (SystematicsPortal.Web.Models.DocumentCitedTaxa citedTaxon in Model.CitedTaxa)
            {
                <label class="col-sm-2 col-form-label">Cited taxa</label>
                <div class="col-sm-10">
                    <a href="">@Html.Raw(citedTaxon.Value)</a>
                </div>
            }
        </div>
    }

    @if (Model.IncludedKeys != null)
    {
        <h4>Identification Key(s)</h4>
        <div class="form-group row">
            @foreach (SystematicsPortal.Web.Models.DocumentIncludedKeys key in Model.IncludedKeys)
            {
                <label class="col-sm-2 col-form-label">Cited taxa</label>
                <div class="col-sm-10">
                    @Html.Raw(key.title)
                </div>
                @foreach (SystematicsPortal.Web.Models.DocumentLine line in key.Line)
                {
                    <label class="col-sm-2 col-form-label">Characteristic</label>
                    <div class="col-sm-10">
                        @Html.Raw(line.Characteristic.Value)
                    </div>
                }
            }
        </div>
    }

    @if (Model.referenceId)
    {
        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Reference Identifier	</label>
            <div class="col-sm-10">
                @Html.Raw(Model.referenceId)
            </div>
        </div>
    }
}

