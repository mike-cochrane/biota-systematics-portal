{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://logger.landcareresearch.co.nz/systematicsdata",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "SystematicsPortal.Web",
      "Environment": "Local"
    }
  },
  "AppSettings": {
    "IdentityServer": {
      "Url": "https://identityserver.landcareresearch.co.nz",
      "ClientId": "",
      "ClientSecret": ""
    },
    "AccessService": {
      "Url": "https://api-access-systematicsportal.landcareresearch.co.nz/",
      "Name": "SystematicsPortal.Web.Api"
    },
    "ContentService": {
      "Url": "https://dev-api-access-annotations.landcareresearch.co.nz/",
      "Name": "Annotations.Web.Api.Access"
    },
    "Email": {
      "SenderAddress": "",
      "SMTPServer": "smtp.landcareresearch.co.nz",
      "ToAddress": "informaticssupport@landcareresearch.co.nz"
    }
  }
}
