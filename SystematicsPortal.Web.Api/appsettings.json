{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://logger.landcareresearch.co.nz/systematicsdata",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "SystematicsPortal.Web.Api",
      "Environment": "Production"
    }
  },
  "AppSettings": {
    "IdentityServer": {
      "Url": "https://identityserver.landcareresearch.co.nz",
      "ApiName": "SystematicsPortal.Web.Api"
    },
    "Solr": {
      "Url": "http://solr-02:8983/solr/biota-systematics-portal",
      "UserName": "",
      "Password": ""
    }
  },
  "ConnectionStrings": {
    "NamesWeb": "Data Source=SQL-01;MultipleActiveResultSets=true;Initial Catalog=Names_Web;Integrated Security=True;MultipleActiveResultSets=True"
  }
}