{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://logger.landcareresearch.co.nz/systematicsdata",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "SystematicsPortal.Data.Uploader",
      "Environment": "Local"
    }
  },
  "AppSettings": {
    "SourcePath": "D:\\Application Data\\SystematicsData\\Source Xml"
  },
  "ConnectionStrings": {
    "NamesWeb": "Data Source=DEV-SQL-02;MultipleActiveResultSets=true;Initial Catalog=Names_Web;Integrated Security=True;MultipleActiveResultSets=True"
  }
}
