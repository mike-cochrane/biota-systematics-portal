{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://logger.landcareresearch.co.nz/systematicsdata",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "SystematicsPortal.Data.Harvester",
      "Environment": "Local"
    }
  },
  "AppSettings": {
    "ContentService": {
      "Url": "https://test-api-access-annotations.landcareresearch.co.nz/",
      "Name": "Annotations.Web.Api.Access",
      "RabbitMq": {
        "Host": "DEV-MQ-01",
        "VirtualHost": "/",
        "Username": "",
        "Password": ""
      }
    }
  },
  "ConnectionStrings": {
    "NamesWeb": "Data Source=DEV-SQL-02;MultipleActiveResultSets=true;Initial Catalog=Names_Web;Integrated Security=True;MultipleActiveResultSets=True"
  }
}
