<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
		<Description>Web site for the systematics portals.</Description>
		<Authors>Manaaki Whenua Landcare Research</Authors>
		<Company>Manaaki Whenua Landcare Research</Company>
		<PackageTags></PackageTags>
		<Version>0.0.0</Version>
		<InformationalVersion Condition=" '$(BUILD_NUMBER)' == '' ">-</InformationalVersion>
