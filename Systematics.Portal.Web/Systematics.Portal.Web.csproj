		<PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="3.1.3" />
	</ItemGroup>

	<ItemGroup>
		<Folder Include="Views\Download\" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Systematics.Portal.Web.Api.Access.Client\Systematics.Portal.Web.Api.Access.Client.csproj" />
